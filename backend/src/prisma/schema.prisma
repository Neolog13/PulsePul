generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Measurement {
  id        String   @id @default(uuid())
  timestamp DateTime @default(now())
  createdAt DateTime @default(now())
  sap       String
  dap       String
  pulse     String
}

// pnpm prisma format
